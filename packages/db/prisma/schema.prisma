generator client {
    provider = "prisma-client-js"
    output   = "../generated/prisma"
}

datasource db {
    provider = "sqlite"
    url      = env("DATABASE_URL")
}

model User2 {
    id        String   @id @default(cuid())
    name      String?
    email     String?  @unique
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}
